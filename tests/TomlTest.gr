    )
  )
assert sortToml(fromString("foo = [ { bar=\"\\\"{{baz}}\\\"\"} ]\n")) ==
  sortToml(
    Ok(
      TomlTable(
        [("foo", TomlArray([TomlTable([("bar", TomlString("\"{{baz}}\""))])]))]
